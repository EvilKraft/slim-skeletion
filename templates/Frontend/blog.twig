{% extends 'frontend_layout.twig' %}

{% block content %}

<div class="content-area" id="primarybwrap">
    <div class="bwrapsite-main" id="mainbwrap" role="main">

        <div class="mainblogsec section" id="mainblogsec">
            <div class="widget Blog" data-version="1" id="Blog1">
                <div class="blog-posts hfeed">
                    <div class="status-msg-wrap">
                        <div class="status-msg-body">Showing posts with label <b>Advertisement</b>.</div>
                        <div class="status-msg-border">
                            <div class="status-msg-bg">
                                <div class="status-msg-hidden">Showing posts with label <b>Advertisement</b>.</div>
                            </div>
                        </div>
                    </div>
                    <div style="clear: both;"></div>
                    <!--Can't find substitution for tag [defaultAdStart]-->

                    <div class="date-outer">
                        <div class="date-posts">


                            <div class="post-outer">
                                <article class="post hentry">
                                    <a content="https://1.bp.blogspot.com/-Y3jtko20nZA/We42YMNGo4I/AAAAAAAACvA/UEahaZNOpCcC_3llLMX40ARU9MRkCPNMQCLcBGAs/s1600/woman_593141_960_720.jpg"></a>
                                    <div class="format-image hentry post-image post-format-image">
                                        <a href="https://news16-templatemark.blogspot.com/2017/10/new-form-of-farming-revolutionizes-way.html" title="New Form of Farming Revolutionizes the Way We Eat">
                                            <img alt="New Form of Farming Revolutionizes the Way We Eat" itemprop="image" src="https://1.bp.blogspot.com/-Y3jtko20nZA/We42YMNGo4I/AAAAAAAACvA/UEahaZNOpCcC_3llLMX40ARU9MRkCPNMQCLcBGAs/s1600/woman_593141_960_720.jpg" style="width:250px; height:200px" title="New Form of Farming Revolutionizes the Way We Eat">
                                        </a>
                                    </div>
                                    <h2 class="post-title entry-title">
                                        <a href="https://news16-templatemark.blogspot.com/2017/10/new-form-of-farming-revolutionizes-way.html" title="New Form of Farming Revolutionizes the Way We Eat">
                                            New Form of Farming Revolutionizes the Way We Eat
                                        </a>
                                    </h2>
                                    <div class="post-info">
                                        <span class="author-info">
                                            <span class="vcard" itemprop="author" itemscope="itemscope" itemtype="http://schema.org/Person">
                                                <span class="fn">
                                                 by <a class="g-profile" href="https://www.blogger.com/profile/09280593907688002943" rel="author" title="author profile"><span itemprop="name">Muhammad Subel</span></a>
                                                </span>
                                            </span>
                                        </span>
                                        <span><i aria-hidden="true" class="fa fa-angle-right"></i></span>
                                        <span class="time-info">
                                            on <a class="timestamp-link" href="https://news16-templatemark.blogspot.com/2017/10/new-form-of-farming-revolutionizes-way.html" rel="bookmark" title="permanent link"><abbr class="published updated timeago" title="2017-10-23T11:35:00-07:00">October 23, 2017</abbr></a>
                                        </span>
                                    </div>
                                    <div class="post-header">
                                        <div class="post-header-line-1"></div>
                                    </div>
                                    <div class="post-body entry-content" id="post-body-8449873593006213023">
                                        <div id="summary8449873593006213023">
                                            <div>
                                                By 2050, the world population will reach almost 10 billion people, at least 66 percent of whom will...
                                            </div>
                                        </div>

                                        <div class="share-post">
                                            <a class="readmore2" href="https://news16-templatemark.blogspot.com/2017/10/new-form-of-farming-revolutionizes-way.html#more" title="New Form of Farming Revolutionizes the Way We Eat">Continue Reading <i aria-hidden="true" class="fa fa-long-arrow-right"></i></a>
                                        </div>
                                        <div style="clear: both;"></div>
                                    </div>
                                </article>
                                <div style="clear:both;"></div>
                            </div>



                            {% for item in items %}
                                <div class="post-outer">
                                    <article class="post hentry">
                                        <a content="{{ path_for('image', {'image_name':item.file}) }}"></a>
                                        <div class="format-image hentry post-image post-format-image">
                                            <a href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" title="{{ item.title }}">
                                                <img itemprop="image" src="{{ path_for('image', {'image_name':item.file}) }}" alt="{{ item.title }}" title="{{ item.title }}" style="width:250px; height:200px">
                                            </a>
                                        </div>
                                        <h2 class="post-title entry-title">
                                            <a href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" title="{{ item.title }}">
                                                {{ item.title }}
                                            </a>
                                        </h2>
                                        <div class="post-info">
                                            <span class="author-info">
                                                <span class="vcard" itemprop="author" itemscope="itemscope" itemtype="http://schema.org/Person">
                                                    <span class="fn">
                                                     by <a class="g-profile" href="https://www.blogger.com/profile/09280593907688002943" rel="author" title="author profile"><span itemprop="name">Muhammad Subel</span></a>
                                                    </span>
                                                </span>
                                            </span>
                                            <span><i aria-hidden="true" class="fa fa-angle-right"></i></span>
                                            <span class="time-info">
                                                on <a class="timestamp-link" href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" rel="bookmark" title="permanent link"><abbr class="published updated timeago" title="2017-10-23T11:35:00-07:00">{{ item.createdAt }}</abbr></a>
                                            </span>
                                        </div>
                                        <div class="post-header">
                                            <div class="post-header-line-1"></div>
                                        </div>
                                        <div class="post-body entry-content">
                                            <div>
                                                <div>{{ item.text|striptags|truncate(30, true) }}</div>
                                            </div>

                                            <div class="share-post">
                                                <a class="readmore2" href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" title="{{ item.title }}">{{ 'Continue Reading'|trans }} <i aria-hidden="true" class="fa fa-long-arrow-right"></i></a>
                                            </div>
                                            <div style="clear: both;"></div>
                                        </div>
                                    </article>
                                    <div style="clear:both;"></div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>

                    <!--Can't find substitution for tag [adEnd]-->
                </div>
                <div style="clear: both;"></div>

                {{ pagination }}

                <div class="blog-pager" id="blog-pager">
                    <span id="blog-pager-older-link">
                        <a class="blog-pager-older-link" href="https://news16-templatemark.blogspot.com/search/label/Advertisement?updated-max=2017-10-23T11:22:00-07:00&amp;max-results=5&amp;start=5&amp;by-date=false" id="Blog1_blog-pager-older-link" title="Older Posts">Older Posts&nbsp;â†’</a>
                    </span>
                    <a class="home-link" href="https://news16-templatemark.blogspot.com/">Home</a>
                </div>
                <div class="clear"></div>
            </div>
        </div>
    </div>
</div>


{% endblock %}

