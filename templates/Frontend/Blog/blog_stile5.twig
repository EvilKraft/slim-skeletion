<div class="mag-wrapper">
    <div class="mag section section" id="recent1">
        <div class="widget HTML" data-version="1" id="HTML923">
            <div class="widget-content">
                <div class="title">
                    <a href="{{ path_for('blog', {'lang':lang, 'id':industry.industryId}) }}">{{ industry.name }}</a>
                    <div class="viewall">
                        <a href="{{ path_for('blog', {'lang':lang, 'id':industry.industryId}) }}">
                            <i aria-hidden="true" class="fa fa-list-ul"></i> {{ 'All News'|trans }}
                        </a>
                    </div>
                </div>
                <div class="clear"></div>
                <ul id="label_with_thumbs">
                    {% for item in items %}
                        <li class="recent-box">
                            <div class="imageContainer">
                                <a href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" target="_top">
                                    <img class="label_thumb" src="{{ path_for('image', {'image_name':item.file}, {'p':'bs5'}) }}" title="{{ item.title }}" alt="{{ item.title }}" style="width: auto; height: 170px; margin-left: -51.5455px; margin-top: 0px;">
                                </a>
                            </div>
                            <a class="label_title" href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" target="_top">{{ item.title }}</a>
                            <div class="toe">
                                <a href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" class="post-date">{{ item.createdAt|date("d.m.Y H:i:s") }}</a>
                            </div>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div></div>
    <div class="clear"></div>
</div>
