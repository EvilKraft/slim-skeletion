<div class="banner section section" id="recent3">
    <div class="widget HTML" data-version="1" id="HTML106">
        <div class="mag-wrapper">
            <div class="widget-content">
                <div class="title">
                    <a href="{{ path_for('blog', {'lang':lang, 'id':industry.industryId}) }}">{{ industry.name }}</a>
                    <div class="viewall">
                        <a href="{{ path_for('blog', {'lang':lang, 'id':industry.industryId}) }}">
                            <i aria-hidden="true" class="fa fa-list-ul"></i> {{ 'All News'|trans }}
                        </a>
                    </div>
                </div>
                <div class="clear"></div>
                <ul id="label_with_thumbs">
                    {% for item in items %}
                        <li class="recent-box">
                            <div class="imageContainer">
                                <a href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" target="_top">
                                    <img class="label_thumb" src="{{ path_for('image', {'image_name':item.file}, {'p':'bs4'}) }}" title="{{ item.title }}" alt="{{ item.title }}" style="width: auto; height: 120px; margin-left: -39.0909px; margin-top: 0px;">
                                </a>
                            </div>
                            <a class="label_title" href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" target="_top">{{ item.title }}</a>
                            <div class="toe">
                                <a href="{{ path_for('post', {'lang':lang, 'id':item.postId}) }}" class="post-date">{{ item.createdAt }}</a>
                            </div>
                            <p class="post-summary">{{ item.text|striptags|truncate(30, true) }}</p>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
</div>
