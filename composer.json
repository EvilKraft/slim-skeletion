{
    "name": "EvilKraft/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/EvilKraft",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Kaluzhnikov",
            "email": "k.kaluzhnikov@gmail.com",
            "homepage": "http://github.com/EvilKraft"
        }
    ],
    "require": {
        "php": ">=5.5.0",

        "oomphinc/composer-installers-extender": "^1.1",
        
        "bower-asset/bootstrap": "^3.3",
        "npm-asset/jquery": "^3.2",
        "npm-asset/jquery-migrate": "^3",
        "bower-asset/jquery-timeago": "^1.6",
        "bower-asset/adminLTE": "^2.3",
        "bower-asset/jquery-toast-plugin": "^1.3",
        "bower-asset/bootstrap-fileinput": "^4.4",
        "bower-asset/bootstrap-star-rating": "^4.0",
        "bower-asset/bootstrap-validator": "^0.11.9",
        "bower-asset/select2": "^4.0",
        "bower-asset/select2-bootstrap-theme-dist": "master@dev",
        "bower-asset/font-awesome": "^4.7",
        "bower-asset/magnific-popup": "^1.1",
        "bower-asset/echarts": "^3.7",
        "bower-asset/moment": "^2.19",
        "bower-asset/i18next": "^10.2",
        "bower-asset/i18next-xhr-backend": "^1.5",
        "bower-asset/ionicons": "master@dev",
        "bower-asset/flag-icon-css": "^2.9",
        "bower-asset/slimscroll": "^1.3",
        "bower-asset/fastclick": "^1.0",
        "bower-asset/animate-css": "^3.5",


        "EvilKraft/dataTableSelectAllBtn": "master@dev",
        "EvilKraft/dataTableActionBtns": "master@dev",
        "EvilKraft/dataTableColumnFilter": "master@dev",
        "EvilKraft/dataTableExportCollection": "master@dev",

        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.1",
        "slim/flash": "^0.1.0",
        "twig/extensions": "^1.4",
        "symfony/translation": "^3.1",
        "symfony/twig-bridge": "^3.1",
        "symfony/config": "^3.1",
        "swiftmailer/swiftmailer": "^6.0",
        "xandros15/slim-pagination": "^0.1.0",
        "rappasoft/laravel-helpers": "dev-master",
        "runcmf/runtracy": "^0.2.10",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset", "my-js"],
        "installer-paths": {
            "public/assets/{$name}/": ["type:bower-asset", "type:npm-asset", "type:my-js"]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type":"package",
            "package": {
                "name": "EvilKraft/dataTableSelectAllBtn",
                "version":"master",
                "type": "my-js",
                "source": {
                    "url": "https://github.com/EvilKraft/dataTableSelectAllBtn.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "EvilKraft/dataTableActionBtns",
                "version":"master",
                "type": "my-js",
                "source": {
                    "url": "https://github.com/EvilKraft/dataTableActionBtns.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "EvilKraft/dataTableColumnFilter",
                "version":"master",
                "type": "my-js",
                "source": {
                    "url": "https://github.com/EvilKraft/dataTableColumnFilter.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "EvilKraft/dataTableExportCollection",
                "version":"master",
                "type": "my-js",
                "source": {
                    "url": "https://github.com/EvilKraft/dataTableExportCollection.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ]
}
